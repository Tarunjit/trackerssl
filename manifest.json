{
  "manifest_version": 2,
  "name": "Tracker SSL",
  "version": "0.0.9",
  "description": "Monitors and tests known ad trackers for HTTPS supprt",
  "icons": {
      "16": "img/trackerssl16.png",
      "48": "img/trackerssl48.png",
      "128": "img/trackerssl128.png"
    },
  "background": {
    "scripts": [
      "app/bower_components/underscore/underscore-min.js", 
      "app/bower_components/backbone/backbone.js",
      "app/bower_components/URIjs/src/URI.min.js",
      "app/https-everywhere/util.js", 
      "app/https-everywhere/lru.js", 
      "app/https-everywhere/rule_list.js", 
      "app/https-everywhere/rules.js", 
      "app/https-everywhere/https_e_bg.js",
      "app/twitter-list.js",
      "background.js"
    ]
  },
  "sandbox": {
    "pages": ["templater.html"]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon19.png",
      "38": "img/icon38.png"
    },
    "default_title": "Tracker SSL",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking", 
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "disconnect.json"
  ]
}